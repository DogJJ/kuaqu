<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleUi">
        <attr name="titleText" format="string" />
        <attr name="backVisiable" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CircleView">
        <attr name="circle_color" format="color" />
    </declare-styleable>
    <declare-styleable name="CheckView">
        - <!--  随机点数
  -->
        <attr name="point_num" format="integer" />
        - <!--  随机线数
  -->
        <attr name="line_num" format="integer" />
        - <!--  验证码长度
  -->
        <attr name="text_length" format="integer" />
        - <!--  验证码字体大小
  -->
        <attr name="text_size" format="dimension" />
        - <!--  验证码字体颜色
  -->
        <attr name="text_color" format="color" />
        - <!--  背景颜色
  -->
        <attr name="bg_color" format="color" />
    </declare-styleable>

</resources>